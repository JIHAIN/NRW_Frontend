{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://alain.r-e.kr/api/$1"
    },
    {
      "src": "/async/(.*)",
      "dest": "https://alain.r-e.kr/async/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
