{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://alain.r-e.kr/api/:path*"
    },
    {
      "source": "/async/:path*",
      "destination": "https://alain.r-e.kr/async/:path*"
    },
    {
      "source": "/((?!api/|async/|_next/|static/|favicon.ico|sw.js).*)",
      "destination": "/index.html"
    }
  ]
}
